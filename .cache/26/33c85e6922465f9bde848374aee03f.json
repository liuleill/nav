ine":64,"column":4}},{"generated":{"line":59,"column":0},"source":"main.js","original":{"line":65,"column":4}},{"name":"localStorage","generated":{"line":61,"column":0},"source":"main.js","original":{"line":66,"column":4}},{"name":"localStorage","generated":{"line":61,"column":2},"source":"main.js","original":{"line":66,"column":4}},{"generated":{"line":61,"column":14},"source":"main.js","original":{"line":66,"column":16}},{"name":"setItem","generated":{"line":61,"column":15},"source":"main.js","original":{"line":66,"column":17}},{"generated":{"line":61,"column":22},"source":"main.js","original":{"line":66,"column":4}},{"generated":{"line":61,"column":23},"source":"main.js","original":{"line":66,"column":25}},{"generated":{"line":61,"column":26},"source":"main.js","original":{"line":66,"column":4}},{"name":"string","generated":{"line":61,"column":28},"source":"main.js","original":{"line":66,"column":29}},{"generated":{"line":61,"column":34},"source":"main.js","original":{"line":66,"column":4}},{"generated":{"line":62,"column":0},"source":"main.js","original":{"line":67,"column":1}},{"generated":{"line":62,"column":1},"source":"main.js","original":{"line":54,"column":0}}],"sources":{"main.js":"const $siteList = $('.siteList')\r\nconst $lastLi = $siteList.find('li.last')\r\n//一进入当前页面的时候获取x值，\r\nconst x = localStorage.getItem('x')\r\nconst xObject = JSON.parse(x) //JSON.parse把字符串x变成对象xObject\r\nconsole.log('x:' + x)\r\nconsole.log('xObject:' + xObject)\r\n//初始化的时候，x有值的时候，xObject是空，所以用 ||来获取后面的数组值\r\nconst hashMap = xObject || [//声明一个全局变量\r\n    {logo:'A',logoType:'text',url:'https://www.acfun.cn'},\r\n    {logo:'B',logoType:'image',url:'https://bilibili.com'},\r\n]\r\n\r\nconst simplifyUrl = (url) =>{\r\n    return url.replace('https://','')\r\n           .replace('http://','')\r\n           .replace('www.','')\r\n           .replace(/\\/.*/,'')\r\n}\r\n\r\nconst render = () => {\r\n    hashMap.forEach(node=>{\r\n        const $li = $(`\r\n                <li>\r\n                    <a href=\"${node.url}\" >\r\n                        <div class=\"site\">\r\n                            <div class=\"logo\">${simplifyUrl(node.url)[0].toUpperCase()}</div>\r\n                            <div class=\"link\">${simplifyUrl(node.url)}</div>\r\n                        </div>\r\n                    </a>\r\n                </li>\r\n        `).insertBefore($lastLi)\r\n    })\r\n}\r\n\r\nrender()\r\n\r\n$('.addButton').on('click',()=>{\r\n    console.log(1)\r\n    let url=window.prompt(\"你想干啥？\")\r\n    if(url.indexOf('https')!==0){\r\n        url = 'https://' + url\r\n    }\r\n    hashMap.push({\r\n        logo:simplifyUrl(url)[0].toUpperCase(),\r\n        logoType:'text',\r\n        url:url\r\n    })\r\n    $siteList.find('li:not(.last)').remove()\r\n    render()\r\n});\r\n\r\n//关闭页面的时候把当前的hashMap存到x里面\r\nwindow.onbeforeunload = () =>{\r\n    console.log(`页面要关闭了`)\r\n    //把对象变成字符串用JSON.stringfy()\r\n    const string = JSON.stringify(hashMap)\r\n    /*查看下面四个值要勾选preserve log*/\r\n    // console.log(hashMap)\r\n    // console.log(typeof hashMap) //object\r\n    // console.log(string)\r\n    // console.log(typeof string) //类型是string\r\n\r\n    //产看localStorage的值，要看控制台中Application\r\n    //中的Storage中的local storage，对应x和string\r\n    localStorage.setItem('x',string)\r\n}"},"lineCount":null}},"error":null,"hash":"8c59b7e7ad062e3fb9a4c457050bf097","cacheData":{"env":{}}}